class Card {
    /**
     * 
     * @param {string} card 
     */
    constructor(card, damage, health, speed, hitSpeed, arena, range, amount, rarity, elixerCost) {
        this.card = card;
        this.damage = damage;
        this.health = health;
        this.speed = speed;
        this.hitspeed = hitSpeed;
        this.arena = arena;
        this.range = range;
        this.amount = amount;
        this.rarity = rarity;
        this.elixerCost = elixerCost;
        /**
         * @type {string}
         */
        this.cardUppercaseName = card.replace(/(^\w|\s\w)/g, m => m.toUpperCase());
    }
};

